@article{kaya_foams_2019,
	title = {Foams of {Gray} {Cast} {Iron} as {Efficient} {Energy} {Absorption} {Structures}: {A} {Feasibility} {Study}},
	volume = {21},
	copyright = {© 2019 WILEY-VCH Verlag GmbH \& Co. KGaA, Weinheim},
	issn = {1527-2648},
	shorttitle = {Foams of {Gray} {Cast} {Iron} as {Efficient} {Energy} {Absorption} {Structures}},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/adem.201900080},
	doi = {10.1002/adem.201900080},
	abstract = {Gray cast iron foams, were produced using a reticulated polyurethane by using a modified investment casting process. To evaluate the attributes of the produced micro-geometries, foam segments and single struts are investigated by light and electron microscopy in 2D, and synchrotron micro-computed tomography in 3D. Mechanical properties are determined by macro/micro-mechanical testing and nanoindentation. In the microstructure of the gray cast iron struts, both flake-like coarse type A and locally fine undercooled type D graphite particles are observed. The distribution of the two graphite types is heterogeneous and is the likely cause for the large scatter of the mechanical properties of the single struts. The high graphite content and the resulting brittle behavior of the struts lead to strong serrations in the stress-strain curve of the foams with a negative effect on energy absorption. We found a relatively low energy absorption efficiency of below 50\% as compared to 75\% in 316L austenitic steel struts. The small specimen size results in scale effects which strongly influence the mechanical properties of the foams. Further improvement in the fabrication of gray cast iron foams is needed to tailor graphite distributions and optimize performance of cast iron-based foams.},
	language = {en},
	number = {6},
	urldate = {2020-02-03},
	journal = {Advanced Engineering Materials},
	author = {Kaya, Ali C. and Zaslansky, Paul and Rack, Alexander and Fischer, Sebastian F. and Fleck, Claudia},
	year = {2019},
	keywords = {cast iron, casting, metal foams, synchrotron micro-computed tomography},
	pages = {1900080},
}

@article{nikolaus_importance_2017,
	title = {Importance of the variable periodontal ligament geometry for whole tooth mechanical function: {A} validated numerical study},
	volume = {67},
	issn = {1751-6161},
	shorttitle = {Importance of the variable periodontal ligament geometry for whole tooth mechanical function},
	url = {https://www.sciencedirect.com/science/article/pii/S1751616116304088},
	doi = {10.1016/j.jmbbm.2016.11.020},
	abstract = {When mammalian teeth breakdown food, several juxtaposed dental tissues work mechanically together, while balancing requirements of food comminution and avoiding damage to the oral tissues. One important way to achieve this is by channeling mastication forces into the surrounding jaw bone through a thin and compliant soft tissue, the periodontal ligament (PDL). As a result, during a typical chewing stroke, each tooth moves quite substantially in its anchor-site. Here we report a series of experiments, where we study the reaction of three-rooted teeth to a single chewing event by finite element (FE) modelling. The nonlinear behaviour of the PDL is simulated by a hyperelastic material model and the in silico results are validated by our own in vitro experiments. We examine the displacement response of the complete tooth-PDL-bone complex to increasing chewing loads. We observe that small spatially-varying geometric adjustments to the thickness of the PDL lead to strong changes in observed tooth reaction movement, as well as PDL strain and bone stress. When reproducing the regionally varying thickness of the PDL observed in vivo, FE simulations reveal subtle but significant tooth motion that leads to an even distribution of the stresses in the jaw bone, and to lower strains in the PDL. Our in silico experiments also reproduce the results of experiments performed by others on different animal models and are therefore useful for overcoming the difficulties of obtaining tooth-PDL-bone loading estimates in vivo. This data thus enhances our understanding of the role the variable PDL geometry plays in the tooth-PDL-bone complex during mastication.},
	language = {en},
	urldate = {2021-11-15},
	journal = {Journal of the Mechanical Behavior of Biomedical Materials},
	author = {Nikolaus, Anneke and Currey, John D. and Lindtner, Tom and Fleck, Claudia and Zaslansky, Paul},
	month = mar,
	year = {2017},
	keywords = {Alveolar bone stress, Finite element analysis, Mastication response, PDL strain, Three-rooted tooth-PDL-bone complex, Tooth displacements},
	pages = {61--73},
}

@article{verhulp_micro-finite_2008,
	title = {Micro-finite element simulation of trabecular-bone post-yield behaviour – effects of material model, element size and type},
	volume = {11},
	issn = {1025-5842},
	url = {https://doi.org/10.1080/10255840701848756},
	doi = {10.1080/10255840701848756},
	abstract = {Micro-finite element (micro-FE) analysis became a standard tool for the evaluation of trabecular bone mechanical properties. The accuracy of micro-FE models for linear analyses is well established. However, the accuracy of recently developed nonlinear micro-FE models for simulations of trabecular bone failure is not known. In this study, a trabecular bone specimen was compressed beyond the apparent yield point. The experiment was simulated using different micro-FE meshes with different element sizes and types, and material models based on cortical bone. The results from the simulations were compared with experimental results to study the effects of the different element and material models. It was found that a decrease in element size from 80 to 40 μm had little effect on predicted post-yield behaviour. Element type and material model had significant effects. Nevertheless, none of the established material models for cortical bone were able to predict the typical descent in the load-displacement curve seen during compression of trabecular bone.},
	number = {4},
	urldate = {2022-08-19},
	journal = {Computer Methods in Biomechanics and Biomedical Engineering},
	author = {Verhulp, E. and Van Rietbergen, B. and Müller, R. and Huiskes, R.},
	month = aug,
	year = {2008},
	pmid = {18568833},
	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.1080/10255840701848756},
	keywords = {bone failure, computed tomography, finite element method, trabecular bone},
	pages = {389--395},
}

