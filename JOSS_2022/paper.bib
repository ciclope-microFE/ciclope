@article{garcia_three-dimensional_2008,
	title = {A three-dimensional elastic plastic damage constitutive law for bone tissue},
	volume = {8},
	issn = {1617-7959, 1617-7940},
	url = {http://link.springer.com/article/10.1007/s10237-008-0125-2},
	doi = {10.1007/s10237-008-0125-2},
	abstract = {Motivated by mechanical analysis of bones and bone-implant systems, a 3D constitutive law describing the macroscopic mechanical behaviour of both cortical and trabecular bone in cyclic (not fatigue) overloads is developed. The proposed model which mathematical formulation is established within the framework of generalized standard materials accounts for three distinct material evolution modes where elastic, plastic and damage aspects are closely related. The anisotropic elasticity of bone is described by a morphology-based model and distinct damage behaviour in tension and compression by a halfspacewise generalized Hill criterion. The plastic criterion is based on the intact elastic compliance tensor. The algorithm applies three distinct projections based on the relationship between the internal variables and criteria. Their respective consistent tangent operators are presented. Numerical resolutions of several boundary value problems and a biomechanical application are presented to illustrate the potential of the constitutive model and demonstrate the expected quadratic convergence of the algorithm.},
	language = {en},
	number = {2},
	urldate = {2016-03-09},
	journal = {Biomechanics and Modeling in Mechanobiology},
	author = {Garcia, David and Zysset, Philippe K. and Charlebois, Mathieu and Curnier, Alain},
	month = apr,
	year = {2008},
	keywords = {Biomedical Engineering, Theoretical and Applied Mechanics, Bone tissue, elasticity, Biophysics/Biomedical Physics, Constitutive law, Damage, Fabric, Plasticity},
	pages = {149--165},
}

